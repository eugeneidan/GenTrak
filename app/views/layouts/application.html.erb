<!DOCTYPE html>
<html>
<head>
  <title>GenTrak</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div class="navbar">
    <ul>
      <li>
         <%= link_to "Check In", checkins_url %> 
      </li>
      <li>
         <%= link_to "Store", items_url %> 
      </li>
      <li>
         <%= link_to "Check Out", checkouts_url %> 
      </li>
    </ul>
  </div>
  <%= yield %>
</div>
</body>
</html>
